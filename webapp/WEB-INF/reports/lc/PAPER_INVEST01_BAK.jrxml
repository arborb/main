	<queryString>
		<![CDATA[SELECT MAX(L1.CENTER_CD_F)      AS CENTER_CD_F
      ,MAX(L1.BU_CD_F)          AS BU_CD_F
      ,MAX(L1.INVEST_DATE)      AS INVEST_DATE
      ,MAX(L1.INVEST_NO)        AS INVEST_NO
      ,MAX(L1.MANAGER_ID)       AS MANAGER_ID
      ,MAX(L1.MANAGER_NM)       AS MANAGER_NM
      ,L1.LOCATION_CD
      ,MAX(L1.BRAND_CD)         AS BRAND_CD
      ,MAX(L1.BRAND_NM)         AS BRAND_NM
      ,L1.ITEM_CD
      ,L1.ITEM_NM
      ,MAX(L1.ITEM_SPEC)        AS ITEM_SPEC
      ,MAX(L1.ITEM_STATE_D)     AS ITEM_STATE_D
      ,MAX(L1.ITEM_LOT)         AS ITEM_LOT
      ,MAX(L1.VALID_DATE)       AS VALID_DATE
      ,MAX(L1.BATCH_NO)         AS BATCH_NO
      ,MAX(L1.QTY_IN_BOX)       AS QTY_IN_BOX
      ,MAX(L1.STOCK_QTY)        AS STOCK_QTY
      ,MAX(L1.STOCK_BOX)        AS STOCK_BOX
      ,MAX(L1.STOCK_EA)         AS STOCK_EA
      ,MAX(L1.INVEST_QTY)       AS INVEST_QTY
      ,MAX(L1.INVEST_BOX)       AS INVEST_BOX
      ,MAX(L1.INVEST_EA)        AS INVEST_EA
  FROM (
          SELECT WF.DSP_COMBO(M1.CENTER_CD ,C1.CENTER_NM)       AS CENTER_CD_F
                ,WF.DSP_COMBO(M1.BU_CD     ,B1.BU_NM)           AS BU_CD_F
                ,M1.INVEST_DATE
                ,M1.INVEST_NO
                ,M1.MANAGER_ID
                ,C3.USER_NM                                     AS MANAGER_NM
                ,M2.LOCATION_CD
                ,M2.BRAND_CD
                ,B2.BRAND_NM
                ,M2.ITEM_CD                                     AS ITEM_CD
                ,T1.ITEM_NM
                ,T1.ITEM_SPEC
                ,C2.CODE_NM                                     AS ITEM_STATE_D
                ,M2.ITEM_LOT
                ,M2.VALID_DATE
                ,M2.BATCH_NO
                ,T1.QTY_IN_BOX
                ,M2.STOCK_QTY
                ,WF.GET_B_BOX(M2.STOCK_QTY ,T1.QTY_IN_BOX)      AS STOCK_BOX
                ,WF.GET_B_EA (M2.STOCK_QTY ,T1.QTY_IN_BOX)      AS STOCK_EA
                ,M2.INVEST_QTY                                  AS INVEST_QTY
                ,WF.GET_B_BOX(M2.INVEST_QTY ,T1.QTY_IN_BOX)     AS INVEST_BOX
                ,WF.GET_B_EA (M2.INVEST_QTY ,T1.QTY_IN_BOX)     AS INVEST_EA
            FROM LC040NM M1
                 JOIN LC040ND M2  ON M1.CENTER_CD   = M2.CENTER_CD
                                 AND M1.BU_CD       = M2.BU_CD
                                 AND M1.INVEST_DATE = M2.INVEST_DATE
                                 AND M1.INVEST_NO   = M2.INVEST_NO
                 JOIN CMCENTER C1 ON C1.CENTER_CD   = M2.CENTER_CD
                 JOIN CMBU     B1 ON B1.BU_CD       = M1.BU_CD
                 JOIN CMBRAND  B2 ON B2.BRAND_CD    = M2.BRAND_CD
                 JOIN CMITEM   T1 ON T1.BRAND_CD    = M2.BRAND_CD
                                 AND T1.ITEM_CD     = M2.ITEM_CD
                 JOIN CMCODE   C2 ON C2.CODE_CD     = M2.ITEM_STATE
                                 AND C2.CODE_GRP    = WF.GET_CODE_GRP('ITEM_STATE')
            LEFT JOIN CSUSER   C3 ON C3.USER_ID     = M1.MANAGER_ID
           WHERE M1.CENTER_CD   = 'G1'
             AND M1.BU_CD       = '5000'
             AND M1.INVEST_DATE = '2014/08/14'
             AND M1.INVEST_NO   = '0003'
           UNION ALL
          SELECT 'G1 - 광주 물류센터'         AS CENTER_CD_F
                ,'5000 - 위메프'              AS BU_CD_F
                ,TO_DATE('2014/08/14','YYYY-MM-DD')           AS INVEST_DATE
                ,'0003'          AS INVEST_NO
                ,NULL           AS MANAGER_ID
                ,NULL           AS MANAGER_NM
                ,M1.LOCATION_CD
                ,NULL           AS BRAND_CD
                ,NULL           AS BRAND_NM
                ,NULL           AS ITEM_CD
                ,NULL           AS ITEM_NM
                ,NULL           AS ITEM_SPEC
                ,NULL           AS ITEM_STATE_D
                ,NULL           AS ITEM_LOT
                ,NULL           AS VALID_DATE
                ,NULL           AS BATCH_NO
                ,0              AS QTY_IN_BOX
                ,0           AS STOCK_QTY
                ,0           AS STOCK_BOX
                ,0           AS STOCK_EA
                ,0           AS INVEST_QTY
                ,0          AS INVEST_BOX
                ,0           AS INVEST_EA
            FROM CMLOCATION M1
        ) L1
      GROUP BY L1.LOCATION_CD
              ,L1.ITEM_CD
              ,L1.ITEM_NM
 ORDER BY LOCATION_CD]]>
	</queryString>
