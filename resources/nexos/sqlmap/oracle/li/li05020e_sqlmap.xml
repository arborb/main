    </parameterMap>
    <procedure id="RS_T1_MASTER" parameterMap="paramRS_T1_MASTER" resultClass="java.util.HashMap">
        { ? = call LI05020E.RS_T1_MASTER(?, ?, ?, ?, ?, ?, ?, ?) }
