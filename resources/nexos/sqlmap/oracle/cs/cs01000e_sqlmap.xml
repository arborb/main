    <!-- [S] - RS_MASTER -->
    <parameterMap id="paramRS_MASTER" class="java.util.Map">
        <parameter property="O_RTN"               mode="OUT" javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR" />
