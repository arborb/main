  <!-- [S] - UPDATE_LC010ND [기타입출고상세내역] -->
  <update id="UPDATE_LC010ND" parameterClass="java.util.Map">
      UPDATE LC010ND
         SET ETC_DIV                  = #P_ETC_DIV#
            ,ETC_COMMENT              = #P_ETC_COMMENT# 
       WHERE CENTER_CD                = #P_CENTER_CD#
         AND BU_CD                    = #P_BU_CD#
         AND ETC_DATE                 = #P_ETC_DATE#
         AND ETC_NO                   = #P_ETC_NO#
         AND LINE_NO                  = #P_LINE_NO#
  </update>
