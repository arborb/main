  <!-- [S] - INSERT_CMITEMLOC [상품별로케이션마스터] -->
  <insert id="INSERT_CMITEMLOC" parameterClass="java.util.Map">
      INSERT INTO CMITEMLOC
                 (
                  CENTER_CD
                 ,BRAND_CD
                 ,ITEM_STATE
                 ,ITEM_CD
                 ,DEPART_CD
                 ,LINE_CD
                 ,CLASS_CD
                 ,ITEM_LOC_DIV
                 ,ZONE_CD
                 ,BANK_CD
                 ,BAY_CD
                 ,LEV_CD
                 <isNotEmpty prepend = "," property= "P_LOCATION_CD"      >LOCATION_CD       </isNotEmpty>
                 ,MIN_UNIT_QTY
                 ,MAX_UNIT_QTY
                 ,FILL_UNIT_QTY
                 ,REG_USER_ID
                 ,REG_DATETIME
                 )
          VALUES (
                  #P_CENTER_CD#
                 ,#P_BRAND_CD#
                 ,#P_ITEM_STATE#
                 ,#P_ITEM_CD#
                 ,#P_DEPART_CD#
                 ,#P_LINE_CD#
                 ,#P_CLASS_CD#
                 ,#P_ITEM_LOC_DIV#
                 ,#P_ZONE_CD#
                 ,#P_BANK_CD#
                 ,#P_BAY_CD#
                 ,#P_LEV_CD#
                 <isNotEmpty prepend = "," property= "P_LOCATION_CD"      >#P_LOCATION_CD#     </isNotEmpty>
                 ,#P_MIN_UNIT_QTY#
                 ,#P_MAX_UNIT_QTY#
                 ,#P_FILL_UNIT_QTY#
                 ,#P_REG_USER_ID#
                 ,SYSDATE
                 )
  </insert>
  <!-- [E] - INSERT_CMITEMLOC [상품별로케이션마스터] -->


  <!-- [S] - UPDATE_CMITEMLOC [상품별로케이션마스터] -->
  <update id="UPDATE_CMITEMLOC" parameterClass="java.util.Map">
      UPDATE CMITEMLOC
         SET LOCATION_CD       = #P_LOCATION_CD#
            ,MIN_UNIT_QTY      = #P_MIN_UNIT_QTY#
            ,MAX_UNIT_QTY      = #P_MAX_UNIT_QTY#
            ,FILL_UNIT_QTY     = #P_FILL_UNIT_QTY#
            ,REG_USER_ID       = #P_REG_USER_ID#
            ,REG_DATETIME      = SYSDATE
       WHERE CENTER_CD         = #P_CENTER_CD#
         AND BRAND_CD          = #P_BRAND_CD#
         AND ITEM_STATE        = #P_ITEM_STATE#
         AND ITEM_CD           = #P_ITEM_CD#
         AND DEPART_CD         = #P_DEPART_CD#
         AND LINE_CD           = #P_LINE_CD#
         AND CLASS_CD          = #P_CLASS_CD#
         AND ITEM_LOC_DIV      = #P_ITEM_LOC_DIV#
         AND ZONE_CD           = #P_ZONE_CD#
         AND BANK_CD           = #P_BANK_CD#
         AND BAY_CD            = #P_BAY_CD#
         AND LEV_CD            = #P_LEV_CD#
  </update>
  <!-- [E] - UPDATE_CMITEMLOC [상품별로케이션마스터] -->


  <!-- [S] - DELETE_CMITEMLOC [상품별로케이션마스터] -->
  <delete id="DELETE_CMITEMLOC" parameterClass="java.util.Map">
      DELETE 
        FROM CMITEMLOC
       WHERE CENTER_CD         = #P_CENTER_CD#
         AND BRAND_CD          = #P_BRAND_CD#
         AND ITEM_STATE        = #P_ITEM_STATE#
         AND ITEM_CD           = #P_ITEM_CD#
         AND DEPART_CD         = #P_DEPART_CD#
         AND LINE_CD           = #P_LINE_CD#
         AND CLASS_CD          = #P_CLASS_CD#
         AND ITEM_LOC_DIV      = #P_ITEM_LOC_DIV#
         <isNotEqual prepend = " AND " property="P_ITEM_LOC_DIV" compareValue="2">
             ZONE_CD           = #P_ZONE_CD#
         AND BANK_CD           = #P_BANK_CD#
         AND BAY_CD            = #P_BAY_CD#
         AND LEV_CD            = #P_LEV_CD#
         </isNotEqual>
  </delete>
  <!-- [E] - DELETE_CMITEMLOC [상품별로케이션마스터] -->


</sqlMap>