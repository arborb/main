<sqlMap namespace="CM01040Q">


    <!--
      //================================================================================================================
      // SELECT SQLMAP
      //================================================================================================================
    -->
    <!-- [S] - RS_MASTER -->
    <parameterMap class="java.util.Map" id="paramRS_MASTER">
        <parameter property="O_RTN"               mode="OUT" javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR" />
        <parameter property="P_CENTER_CD"         mode="IN" />
        <parameter property="P_ZONE_CD"           mode="IN" />
        <parameter property="P_BANK_CD1"          mode="IN" />
        <parameter property="P_BANK_CD2"          mode="IN" />
        <parameter property="P_BAY_CD1"           mode="IN" />
        <parameter property="P_BAY_CD2"           mode="IN" />
        <parameter property="P_LEV_CD1"           mode="IN" />
        <parameter property="P_LEV_CD2"           mode="IN" />
    </parameterMap>
    <procedure id="RS_MASTER" parameterMap="paramRS_MASTER" resultClass="java.util.HashMap">
        { ? = call CM01040Q.RS_MASTER(?, ?, ?, ?, ?, ?, ?, ?) }
    </procedure>
    <!-- [E] - RS_MASTER -->


    <!-- [S] - RS_REPORT -->
    <parameterMap class="java.util.Map" id="paramRS_REPORT">
        <parameter property="O_RTN"               mode="OUT" javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR" />
        <parameter property="P_CENTER_CD"         mode="IN" />
    </parameterMap>
    <procedure id="RS_REPORT" parameterMap="paramRS_REPORT" resultClass="java.util.HashMap">
        { ? = call CM01040Q.RS_REPORT(?) }
    </procedure>
    <!-- [E] - RS_REPORT -->





    <!--
      //================================================================================================================
      // INSERT/UPDATE/DELETE SQLMAP
      //================================================================================================================
    -->


</sqlMap>