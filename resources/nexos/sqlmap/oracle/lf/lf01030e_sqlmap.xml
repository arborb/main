     <!-- [S] - LFAMOUNT [청구/지급 상세 내역] -->
  <insert id="INSERT_LFAMOUNT" parameterClass="java.util.Map">
      INSERT INTO LFAMOUNT_CHARGE
                 (
                  ADJUST_MONTH
                 ,ADJUST_DATE
                 ,ADJUST_NO
                 ,CENTER_CD
                 ,BU_CD
                 ,FEE_HEAD_CD
                 ,FEE_BASE_CD
                 ,BRAND_CD
                 <isNotEmpty prepend = "," property= "P_FEE_HEAD_NM"      >FEE_HEAD_NM       </isNotEmpty>
                 <isNotEmpty prepend = "," property= "P_FEE_BASE_NM"      >FEE_BASE_NM       </isNotEmpty>
                 <isNotEmpty prepend = "," property= "P_PAY_CHA_DIV"      >PAY_CHA_DIV       </isNotEmpty>
                  <isNotEmpty prepend = "," property= "P_UNIT_PRICE"       >UNIT_PRICE        </isNotEmpty>
                 <isNotEmpty prepend = "," property= "P_FEE_QTY"          >FEE_QTY           </isNotEmpty>
                 <isNotEmpty prepend = "," property= "P_FEE_AMT"          >FEE_AMT           </isNotEmpty>
                   <isNotEmpty prepend = "," property= "P_REMARK1"          >REMARK1           </isNotEmpty>
                 ,REG_USER_ID
                 ,REG_DATETIME
                 )
          VALUES (
                  #P_ADJUST_MONTH#
                 ,#P_ADJUST_DATE#
                 ,#P_ADJUST_NO#
                 ,#P_CENTER_CD#
                 ,#P_BU_CD#
                 ,#P_FEE_HEAD_CD#
                 ,#P_FEE_BASE_CD#
                 ,#P_BRAND_CD#
                 <isNotEmpty prepend = "," property= "P_FEE_HEAD_NM"        >#P_FEE_HEAD_NM#   </isNotEmpty>
                 <isNotEmpty prepend = "," property= "P_FEE_BASE_NM"        >#P_FEE_BASE_NM#   </isNotEmpty>
                 <isNotEmpty prepend = "," property= "P_PAY_CHA_DIV"        >#P_PAY_CHA_DIV#   </isNotEmpty>
                  <isNotEmpty prepend = "," property= "P_UNIT_PRICE"         >#P_UNIT_PRICE#    </isNotEmpty>
                 <isNotEmpty prepend = "," property= "P_FEE_QTY"            >#P_FEE_QTY#       </isNotEmpty>
                 <isNotEmpty prepend = "," property= "P_FEE_AMT"            >#P_FEE_AMT#       </isNotEmpty>
                  <isNotEmpty prepend = "," property= "P_REMARK1"            >#P_REMARK1#       </isNotEmpty>
                 ,#P_REG_USER_ID#
                 ,SYSDATE
                 )
  </insert>
