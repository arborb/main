  <update id="UPDATE_LO020NT" parameterClass="java.util.Map">
    UPDATE LO020NT
       SET CARRIER_CD               = #P_CARRIER_CD#         
          ,WB_NO                    = #P_WB_NO#                  
     WHERE CENTER_CD                = #P_CENTER_CD#
       AND BU_CD                    = #P_BU_CD#
       AND OUTBOUND_DATE            = #P_OUTBOUND_DATE#
       AND OUTBOUND_NO              = #P_OUTBOUND_NO#
       AND BOX_NO                   = #P_BOX_NO#
