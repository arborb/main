               )
        VALUES (
                #P_CENTER_CD#
               ,#P_BU_CD#
               ,#P_INVEST_DATE#
               ,#P_INVEST_NO#
               ,#P_LINE_NO#
               <isNotEmpty prepend = "," property= "P_LOCATION_CD"             >#P_LOCATION_CD#            </isNotEmpty>
               <isNotEmpty prepend = "," property= "P_BRAND_CD"                >#P_BRAND_CD#               </isNotEmpty>
