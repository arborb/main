    INSERT INTO LC040ND
               (
                CENTER_CD
               ,BU_CD
               ,INVEST_DATE
               ,INVEST_NO
               ,LINE_NO
               <isNotEmpty prepend = "," property= "P_LOCATION_CD"             >LOCATION_CD                </isNotEmpty>
               <isNotEmpty prepend = "," property= "P_BRAND_CD"                >BRAND_CD                   </isNotEmpty>
